{
    "name": "n8n-nodes-je-sics-adapter",
    "version": "1.0.2-beta.2",
    "description": "N8N node for SICS Flow Adapter with versioning support for different development teams",
    "packageManager": "pnpm@9.5.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "jest",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write src/**/*.ts",
        "mock-server": "node mock-sics-server.js",
        "test-setup": "pnpm run build && pnpm pack",
        "quick-test": "pnpm run build && node quick-test.js"
    },
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "n8n-nodes",
        "sics",
        "flow-adapter",
        "workflow",
        "automation"
    ],
    "author": "Development Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/GAVRILOVGLEB/n8n-nodes-je-sics-adapter.git"
    },
    "homepage": "https://github.com/GAVRILOVGLEB/n8n-nodes-je-sics-adapter#readme",
    "dependencies": {
        "n8n-workflow": "^1.97.0",
        "axios": "^1.6.0",
        "semver": "^7.5.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/semver": "^7.5.0",
        "typescript": "^5.0.0",
        "jest": "^29.0.0",
        "@types/jest": "^29.0.0",
        "ts-jest": "^29.0.0",
        "eslint": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "prettier": "^3.0.0",
        "express": "^4.18.0",
        "cors": "^2.8.5"
    },
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [],
        "nodes": [
            "dist/nodes/SicsAdapter.node.js"
        ]
    },
    "files": [
        "dist/**/*",
        "package.json"
    ]
}